<UserControl x:Class="ComputerGraphics.fract"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ComputerGraphics"
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="1200">
    <Grid>
        <Canvas x:Name="canvas" HorizontalAlignment="Left" Height="680" Margin="10,10,0,0" VerticalAlignment="Top" Width="1180">
            <Button x:Name="button_back" Content="Назад" Width="76" Click="back_Click" IsEnabled="False"/>
            <Button x:Name="button_forward" Content="Вперед" Canvas.Left="81" Width="75" Click="forward_Click"/>
        </Canvas>
    </Grid>
</UserControl>
